pipeline{
agent any
environment {
  buildNumber = "${currentBuild.number}" }
tools{
maven "maven3.9.2"
}
stages{
 stage('checkout'){
  steps{
git url: 'https://github.com/Time-traveling/traveling-maven-web-app.git'

  } // close steps  
 } // close stage

stage('maven build'){
steps {
sh "mvn clean package"
  } // close steps
} //close stage
